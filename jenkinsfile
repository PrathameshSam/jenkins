node{
stage('Ready_SCM') {
	echo "SCM getting ready"
	echo "SCM ready for build"
}
stage('Build') {
	echo "Building"
	try {
		sh 'sh hello_world.sh'
	}
	catch(ex) {
		println(ex)
	}	
	finally }
	//
	}
	echo "Building with newfeature branch"
}
stage('Test') {
	echo "Testing"
}
stage('Deploy'){
	echo "Deploying"
}
}
